<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:paddingLeft="16dp"
              android:paddingRight="16dp"
              android:orientation="vertical"
              android:background="@drawable/knap_graa10_bg">

  <View
      android:id="@+id/stiplet_linje"
      android:background="@drawable/stiplet_linje"
      android:layerType="software"
      android:layout_width="match_parent"
      android:layout_marginBottom="13dp"
      android:layout_height="3dp"/>

  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:paddingBottom="16dp">

    <ImageView
        android:id="@+id/billede"
        android:layout_width="140dp"
        android:scaleType="fitEnd"
        android:layout_marginTop="8dp"
        android:contentDescription="P4" tools:ignore="HardcodedText"
        android:layout_height="wrap_content"
        android:src="@drawable/kanalappendix_p4f"/>


    <TextView
        android:id="@+id/tekst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:text="KÃ¸benhavn"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="8dp"
        android:textAppearance="?android:attr/textAppearance"
        tools:ignore="HardcodedText"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ikon"
        tools:ignore="ContentDescription"/>
  </LinearLayout>
</LinearLayout>
